BoulderDash keys (extract from source : src/keyboard.c)

   /***
    * game keys
    */
   o,	/* KEY_GAME_UP      */
   k,	/* KEY_GAME_DOWN    */
   x,	/* KEY_GAME_LEFT    */
   c,	/* KEY_GAME_RIGHT   */
   0,	/* KEY_GAME_FIRE    */
   q,	/* KEY_GAME_QUIT    */
   s,	/* KEY_GAME_RESET   */
   p,	/* KEY_GAME_PLAYERS */
   t,	/* KEY_GAME_SCORE   */
   e,	/* KEY_GAME_EDITOR  */

   /***
    * common keys
    */
   h,		/* KEY_HELP         */
   space,	/* KEY_SPACE        */
   Return,	/* KEY_ENTER        */
   F1,		/* KEY_SNAPSHOT     */

   /***
    * editor keys
    */
   Up,		/* KEY_EDITOR_UP     */
   Down,	/* KEY_EDITOR_DOWN   */
   Left,	/* KEY_EDITOR_LEFT   */
   Right,	/* KEY_EDITOR_RIGHT  */
   C,		/* KEY_EDITOR_CLEAR  */
   U,		/* KEY_EDITOR_UNDO   */
   L,		/* KEY_EDITOR_LOAD   */
   S,		/* KEY_EDITOR_SAVE   */
   Q,		/* KEY_EDITOR_QUIT   */
   Insert,	/* KEY_EDITOR_INSERT */
   Home,	/* KEY_EDITOR_HOME   */
   End,		/* KEY_EDITOR_END    */
   Prior,	/* KEY_EDITOR_TOP    */
   Next,	/* KEY_EDITOR_BOTTOM */
   Tab,		/* KEY_GAME_EDITOR   */

   /***
    * ????
    */
   XK_1,
   XK_2,
   XK_3,
   XK_4,
   XK_5,
   XK_6,
   XK_7,
   XK_8,
   XK_9,
   XK_0,
   XK_r,
   XK_e


$ BoulderDash -c   Use commodore 64 graphics.
$ BoulderDash -s   Use ZX spectrum graphics.

http://www.linuxunderground.be/boulderdash.html - postmaster@linuxunderground.be